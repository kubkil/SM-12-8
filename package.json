{
  "name": "SM-12-8",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "npm-run-all -p watch:*",
    "watch:sass": "node-sass -w --source-map true --output-style compressed scss/ -o css/",
    "watch:bs": "browser-sync start --server --files css/*.css *.html --tunnel",
    "autoprefix": "postcss css/style.css -u autoprefixer -o css/style.prefixed.css",
    "watch:autoprefix": "onchange \"css/style.css\" -d 50 -- npm run autoprefix",
    "build": "npm-run-all -s build:*",
    "build:css": "node-sass --source-map true --output-style compressed scss/ -o css/",
    "build:autoprefix": "postcss css/style.css -u autoprefixer -o css/style.prefixed.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kubkil/SM-12-8.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kubkil/SM-12-8/issues"
  },
  "homepage": "https://github.com/kubkil/SM-12-8#readme",
  "devDependencies": {
    "autoprefixer": "^8.4.1",
    "browser-sync": "^2.24.4",
    "hoek": "^5.0.3",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "onchange": "^4.0.0",
    "postcss": "^6.0.22"
  },
  "browserslist": [
    "> 0.5%",
    "last 4 versions",
    "Firefox ESR",
    "not dead"
  ],
  "dependencies": {
    "postcss-cli": "^5.0.0"
  }
}
